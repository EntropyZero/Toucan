{
  "version": "1.0.0-*",
  "description": "Toucan Core Abstractions Library",
  "authors": [ "Phil McMillan" ],
  "tooling": {
    "defaultNamespace": "Toucan.Core"
  },  
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.0.0",
      "type": "platform"
    },
    "System.Security.Claims": "4.0.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "portable-net45+wp80+win8+wpa81+dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  
  "packOptions": {

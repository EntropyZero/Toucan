{
  "version": "1.0.0-*",
  "description": "Toucan MVC Resource Authorization Library",
  "authors": [ "Phil McMillan" ],
  "tooling": {
    "defaultNamespace": "Toucan"
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "Toucan.Core": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc": "1.0.0"
  },
  
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "portable-net45+wp80+win8+wpa81+dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  
  "packOptions": {
    "summar": "Toucan MVC Resource Authorization Library",
    "tags": ["MVC", "Authorization", "Rails", "CanCan", "Canard", ""],
    "projectUrl": "https://github.com/EntropyZero/Toucan",
    "licenseUrl": "https://raw.githubusercontent.com/EntropyZero/Toucan/master/LICENSE",
    "releaseNotes": "The direct dependency for EntityFramework was removed by restructuring Toucan to have a core package of abstractions and a separate ORM adapter structure. See the Getting Started section on the project page for more details."
  }
}
